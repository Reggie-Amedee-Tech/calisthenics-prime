)}\n                    </tbody>\n                </table>\n                <button onClick={() => {\n                    deleteRegimen(id)\n                    navigate('/allWorkoutRegimens')\n                }} classname={classes.Button}>Delete Regimen</button>\n            </div>\n        </div>\n    )\n}\n\nexport default DetailedRegimenPage",{"ruleId":"79","severity":1,"message":"80","line":9,"column":22,"nodeType":"81","messageId":"82","endLine":9,"endColumn":33},{"ruleId":"83","severity":1,"message":"84","line":49,"column":8,"nodeType":"85","endLine":49,"endColumn":17,"suggestions":"86"},{"ruleId":"83","severity":1,"message":"87","line":16,"column":6,"nodeType":"85","endLine":16,"endColumn":8,"suggestions":"88"},{"ruleId":"79","severity":1,"message":"89","line":2,"column":10,"nodeType":"81","messageId":"82","endLine":2,"endColumn":21},{"ruleId":"79","severity":1,"message":"90","line":2,"column":23,"nodeType":"81","messageId":"82","endLine":2,"endColumn":34},{"ruleId":"79","severity":1,"message":"91","line":3,"column":8,"nodeType":"81","messageId":"82","endLine":3,"endColumn":28},{"ruleId":"83","severity":1,"message":"92","line":22,"column":8,"nodeType":"85","endLine":22,"endColumn":10,"suggestions":"93"},{"ruleId":"83","severity":1,"message":"92","line":33,"column":8,"nodeType":"85","endLine":33,"endColumn":10,"suggestions":"94"},"no-unused-vars","'setWorkouts' is assigned a value but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'id' and 'navigate'. Either include them or remove the dependency array.","ArrayExpression",["95"],"React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.",["96"],"'useSelector' is defined but never used.","'useDispatch' is defined but never used.","'InititateWorkoutPage' is defined but never used.","React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.",["97"],["